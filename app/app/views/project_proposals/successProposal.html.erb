<%= stylesheet_link_tag "style" %>
<%= stylesheet_link_tag "component" %>
<%= stylesheet_link_tag "flickity/flickity.css" %>
<%= javascript_include_tag "application"%>

<% if @session.nil? %>
    <%= render "layouts/headerLogin" %>
<% else %>
    <%= render "layouts/headerUser", :projects => @projects, :images => @project_coverImgs %>
    <%= yield %>
<% end %>
<div class="page-section">

  <legend class="form-heading heading">Thank you for your submission!</legend>
  <p><strong>We will contact you soon!</strong><br>
    <strong>Do note that you can only edit your project after receiving an email from us</strong></p>
  <br>
  <% if @session.nil? %>
      <%= link_to 'Click here to go back to the home page', root_path %>
  <% else %>
      <%= link_to 'Click here to go back to the home page', dashboardIndex_path %>

  <% end %>

</div>

<%= render "layouts/footer" %>
<%= yield %>